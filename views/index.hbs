<div class="container">
  <div class="row">
    <div class="col s12 m6 offset-m3">
      <ul class="collection">
        {{#each users}}
        <li class="collection-item avatar">
          <a href="/users/{{this.id}}" >
            <img src="assets/{{this.image_url}}" alt="" class="circle">
            <span class="title">{{this.name}}</span>
          </a>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
